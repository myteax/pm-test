{
  "family": "tee1",
  "taskRoleArn": "arn:aws:iam::716591628268:role/ecsTaskExecutionRole",
  "executionRoleArn":"arn:aws:iam::716591628268:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mypm",
      "image": "716591628268.dkr.ecr.us-east-1.amazonaws.com/mypm:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "mypm-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "mypm-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "entryPoint": [
        "sh",
        "-c"
      ],
      "command": [
        "/start.sh"
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072"
}
